{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#21c7a8",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\uf179 ",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
            "ubuntu": "\uf31b ",
            "windows": "\uf871 ",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "style": "plain",
          "type": "os"
        },
        {
          "foreground": "#E06C75",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "template": " <#ffffff>\uf63d</> \uf292 root"
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#55B9C4",
          "properties": {
            "style": "folder",
            "template": " <#ffffff>\uf63d</> {{if eq .Path \".git\" }}\uf7a3{{ else }}\ue5fe{{end}} {{ .Path }}"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#fffb38",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_icon": "\uf418 ",
            "commit_icon": "\uf431 ",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "template": " <#ffffff>on</> {{ if .UpstreamIcon }}{{ .UpstreamIcon }} {{ end }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#98C379",
          "properties": {
            "fetch_version": true,
            "template": " <#ffffff>\uf63d</> \uE718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}"
          },
          "style": "plain",
          "type": "node"
        },
        {
          "foreground": "#FFDE57",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false,
            "template": " <#ffffff>\uf63d</> \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "plain",
          "type": "python"
        },
        {
          "foreground": "#4063D8",
          "powerline_symbol": "\uE0b0",
          "properties": {
            "template": " \ue73d {{ .Full }} "
          },
          "type": "php",
          "style": "powerline"
        },
        {
          "foreground": "#C94A16",
          "properties": {
            "template": " <#ffffff>\uf63d</> \uf65b "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#9b22e6",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true,
            "template": "\uf64f {{ .FormattedMs }} <#ffffff>\uf142</> "
          }
        },
        {
          "type": "battery",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "#f0f0f0",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#ffe600{{end}}",
            "{{if eq \"Discharging\" .State.String }}{{ if gt .Percentage 40 }}#f0f0f0{{ else if gt .Percentage 10 }}#ee7300{{ else }}#ee1400{{end}}{{end}}",
            "{{if eq \"Full\" .State.String}}#00be49{{end}}"
          ],
          "properties": {
            "template": "{{ if not .Error }}{{ .Icon }}{{ if eq \"Charging\" .State.String }}\uf583{{ else if eq \"Full\" .State.String }}\uf58e{{ else }}{{ if gt .Percentage 89 }}\uf578{{ else if gt .Percentage 69 }}\uf57f{{ else if gt .Percentage 49 }}\uf57d{{ else if gt .Percentage 10 }}\uf57b{{ else }}\uf582{{ end }}{{ end }} {{ .Percentage }}%{{ end }}"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#63F08C",
          "properties": {
            "template": "\uf63d"
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 1
}
